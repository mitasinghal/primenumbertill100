public class pattern {
	public static void main( String arg[]){
        for(int i=1;i<=4;i++){
 
            for(int j=1;j<=i;j++){
                System.out.print(j);
            }
            System.out.println();
        }
           for(int i=1,r=5;i<=5;i++,r--){
            for(int j=1;j<=r;j++){
                System.out.print(j);
            }
            System.out.println();        }
    }
}
